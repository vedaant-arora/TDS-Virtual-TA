{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "ignore": [
    "downloaded_threads/*",
    "markdown_files/*",
    "__pycache__/*",
    "*.db-journal",
    "*.log",
    "*.tmp"
  ]
}